{
	"manifest_version": 2,
	"name": "Youtube BG",
	"description": "Youtube background playback for Kiwi Browser",
	"version": "1",
	"content_scripts": [
		{
		  "matches": ["*://*.youtube.com/*",
					  "*://*.youtube-nocookie.com/*"],
		  "js": ["content.js"],
		  "all_frames": true
		}
    ],
	"icons": {
		"64": "icon.png"
	}
}
